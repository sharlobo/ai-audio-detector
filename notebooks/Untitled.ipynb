    "import os\n",
    "\n",
    "folder = \"deepfake_audio_project/data/processed_embeddings/\"\n",
    "\n",
    "for filename in os.listdir(folder):\n",
    "    if filename.endswith(\".pt\"):\n",
    "        os.remove(os.path.join(folder, filename))\n",
    "\n",
    "print(\"Deleted all old .pt files.\")\n"
